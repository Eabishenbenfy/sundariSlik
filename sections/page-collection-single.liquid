{% stylesheet %}
:root{
  --fontsize-title: 32px;
}
  .shopify-section.sundarisilks__singleCollection--promotion {
    margin-bottom: -80px;
  }.shopify-section.sundarisilks__singleCollection--promotion {
  margin-bottom: -80px;
}
.shopify-section.sundarisilks__singleCollection--promotion .sundarisilks__singleCollection-page h1,
.shopify-section.sundarisilks__singleCollection--promotion .sundarisilks__singleCollection-page h2,
.shopify-section.sundarisilks__singleCollection--promotion .sundarisilks__singleCollection-page h3,
.shopify-section.sundarisilks__singleCollection--promotion .sundarisilks__singleCollection-page p {
  padding: 0;
  margin: 0;
}
.shopify-section.sundarisilks__singleCollection--promotion .sundarisilks__singleCollection-page {
  background: url("https://sundarisilks.com/cdn/shop/t/33/assets/pattern.png"), #fae0c0;
  background-repeat: repeat;
}
.shopify-section.sundarisilks__singleCollection--promotion .sigle-collection-banner {
  border-radius: 0px 0px 100px 100px;
}
.shopify-section.sundarisilks__singleCollection--promotion .sundarisilks__singleCollection-page .container {
  padding: 40px 14px;
  max-width: 1200px;
  margin: 0 auto;
}
.shopify-section.sundarisilks__singleCollection--promotion .sigle-collection-banner img {
  width: 100%;
  height: auto;
}
.shopify-section.sundarisilks__singleCollection--promotion .section-title p {
  max-width: 794px;
  text-align: center;
  margin: 0 auto;
  padding-bottom: 16px;
}
.shopify-section.sundarisilks__singleCollection--promotion .section-title h2 {
  font-family: "Playfair Display";
  font-style: normal;
  font-weight: 400;
  font-size: 32px;
  line-height: 43px;
  text-align: center;
  color: #000000;
  /* padding-bottom: 24px; */
}
.shopify-section.sundarisilks__singleCollection--promotion .section-title .section-title-divider {
  text-align: center;
  margin: 0 auto;
  padding-top: 8px;
  padding-bottom: 20px;
}
.shopify-section.sundarisilks__singleCollection--promotion .section-title-divider img {
  margin: 0 auto;
}
.shopify-section.sundarisilks__singleCollection--promotion .container.about--content__wrapper {
  background: linear-gradient(94.86deg, #f4c099 0%, #f3c07b 50%, #f2bf74 100%);
  border-radius: 16px;
  display: flex;
  max-height: 450px;
  align-items: flex-end;
  justify-content: center;
  padding-top: 0;
  padding-bottom: 0;
  margin: 80px auto 0;
}
.shopify-section.sundarisilks__singleCollection--promotion .about--content {
  max-width: 590px;
  text-align: center;
  padding: 124px 0;
}
.shopify-section.sundarisilks__singleCollection--promotion .about--content p {
  font-family: "Playfair Display";
  font-style: normal;
  font-weight: 400;
  font-size: var(--fontsize-title);
  line-height: normal;
  text-align: center;
  color: #885519;
  padding-bottom: 16px;
}
.shopify-section.sundarisilks__singleCollection--promotion .about--content a {
  display: inline-block;
  padding: 16px 24px;
  background: #ffffff;
  border-radius: 6px;
  font-family: "Montserrat";
  font-style: normal;
  font-weight: 500;
  font-size: 14px;
  line-height: 17px;
  text-align: center;
  letter-spacing: 0.1em;
  color: #885519;
}
.shopify-section.sundarisilks__singleCollection--promotion .product__list {
  display: grid;
  grid-template-columns: 1fr 1fr;
  gap: 20px;
}
@media (min-width: 768px) {
  .shopify-section.sundarisilks__singleCollection--promotion .product__list {
    grid-template-columns: 1fr 1fr 1fr 1fr;
  }
}
.shopify-section.sundarisilks__singleCollection--promotion .product__card--image {
  /* background-image: url('https://sundarisilks.com/cdn/shop/t/33/assets/border-image.png'); */
  /* background-size: contain; */
  /* background-repeat: no-repeat; */
  /* padding: 10px; */
  border: 10px solid #f3ecdc;
  overflow: hidden;
  border-radius: 0px 0px 40px 40px;
  margin-bottom: 8px;
  width: 100%;
  /* min-height: 388px; */
}
.shopify-section.sundarisilks__singleCollection--promotion .product__card--title {
  padding: 0;
  margin: 0;
  font-family: "Montserrat";
  font-style: normal;
  font-weight: 400;
  font-size: 14px;
  line-height: 17px;
  text-align: center;
  color: #000000;
  display: inline-block;
  margin-bottom: 8px;
  overflow: hidden;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}
.shopify-section.sundarisilks__singleCollection--promotion .product__card--price {
  font-family: "Montserrat";
  font-style: normal;
  font-weight: 600;
  font-size: 14px;
  line-height: 17px;
  text-align: center;
  font-feature-settings: "pnum" on, "lnum" on;
  color: #000000;
}
.shopify-section.sundarisilks__singleCollection--promotion .container.about__collection--special {
  position: relative;
  z-index: 1;
  padding-top: 80px;
  padding-bottom: 80px;
}
.shopify-section.sundarisilks__singleCollection--promotion .about__collection--special:before {
  content: "";
  display: block;
  position: absolute;
  top: 0;
  bottom: 0;
  left: -9999em;
  right: -9999em;
  z-index: -1;
  background-color: #feedd9;
}
.shopify-section.sundarisilks__singleCollection--promotion .blog__lists {
  display: grid;
  grid-template-columns: 1fr 1fr;
  gap: 20px;
}
@media (min-width: 768px) {
  .shopify-section.sundarisilks__singleCollection--promotion .blog__lists {
    grid-template-columns: 1fr 1fr 1fr;
  }
}
.shopify-section.sundarisilks__singleCollection--promotion .blog__list {
  border: 1px solid #c2964b;
  border-radius: 0px 0px 40px 40px;
  padding-bottom: 40px;
}
.shopify-section.sundarisilks__singleCollection--promotion .blog__list--image {
  margin-bottom: 20px;
}
.shopify-section.sundarisilks__singleCollection--promotion .blog__list--title {
  padding-left: 16px;
  padding-right: 16px;
  font-family: "Montserrat";
  font-style: normal;
  font-weight: 700;
  font-size: 20px;
  line-height: 24px;
  color: #000000;
  margin-bottom: 8px;
}
.shopify-section.sundarisilks__singleCollection--promotion .blog__list--date {
  padding-left: 16px;
  padding-right: 16px;
  font-family: "Montserrat";
  font-style: normal;
  font-weight: 400;
  font-size: 16px;
  line-height: 24px;
  color: #000000;
  margin-bottom: 20px;
}
.shopify-section.sundarisilks__singleCollection--promotion .blog__list--content {
  padding-left: 16px;
  padding-right: 16px;
  font-family: "Montserrat";
  font-style: normal;
  font-weight: 400;
  font-size: 14px;
  line-height: 20px;
  color: #000000;
  margin-bottom: 20px;
}
.shopify-section.sundarisilks__singleCollection--promotion .blog__list--link-text {
  padding-left: 16px;
  padding-right: 16px;
  font-family: "Montserrat";
  font-style: normal;
  font-weight: 500;
  font-size: 16px;
  line-height: 20px;
  text-decoration-line: underline;
}
.shopify-section.sundarisilks__singleCollection--promotion .blog__list--link {
  display: inline-block;
}

.shopify-section.sundarisilks__singleCollection--promotion .feature__list ul {
  list-style: none;
  display: flex;
  justify-content: center;
  align-items: center;
}

.shopify-section.sundarisilks__singleCollection--promotion .feature__list li.feature__card{
  padding: 16px 24px;
  text-align: center;
    margin: 0 auto;
}
.shopify-section.sundarisilks__singleCollection--promotion .feature__list svg {
  width: 60px;
  height: 60px;
  margin-bottom: 16px;
}
.shopify-section.sundarisilks__singleCollection--promotion .feature__list .feature__card--content {
  font-family: "Montserrat";
  font-style: normal;
  font-weight: 500;
  font-size: 16px;
  line-height: 20px;
  text-align: center;
  color: #000000;
}

{% endstylesheet %}

<div class="sundarisilks__singleCollection-page">
  <section class="">
    <div class="sigle-collection-banner">
      <picture>
        <source
          media="(max-width: 768px)"
          srcset="{{ 'single-collection-banner-mobile.png' | asset_url }}"
          type="image/png"
        >
        <img
          src="{{ 'single-collection-banner.png' | asset_url }}"
          alt="Description of the image"
          width="1440"
          height="550"
        >
      </picture>
    </div>
  </section>

  <section class="container">
    <div class="section-title">
      <h2>New Arrivals of Soft Silks</h2>
      <div class="section-title-divider">
        <img
          src="{{ 'section-title-divider.svg' | asset_url }}"
          alt="Section Title Divider"
          width="392"
          height="40"
        >
      </div>
    </div>
    <div class="product__list">
      {%- assign collection_handle = 'contemporary-soft-silk' -%}
      {%- assign collection = collections[collection_handle] -%}
      {% for product in collection.products limit: 4 %}
        {% if product.available %}
          <div class="prodct__card">
            <a href="{{ product.url | within: collection }}" class="product__card--link">
              {% if product.featured_image != blank %}
                <img
                  loading="lazy"
                  class="product__card--image lazyload"
                  src="{{ product.featured_image | image_url: width: '288x'  }}"
                  width="288"
                  height="auto"
                  data-aspectratio="{{ product.featured_image.aspect_ratio }}"
                  alt="
                    {% if  product.feature_image.alt != blank %}
                    {{ product.feature_image.alt }}
                    {% else %}
                    {{ product.title }}
                    {% endif %}
                  "
                >
                {% comment %}
                {% endcomment %}
              {% else %}
                {{ 'product-1' | placeholder_svg_tag: 'product-img' }}
              {% endif %}
              <h3 itemprop="name" class="product__card--title">{{ product.title }}</h3>

              {% if product.price_varies %}
                <p class="product__card--price">
                  From {{ product.price_min | money_with_currency }} to
                  {{ product.price_max | money_without_currency }}
                </p>
              {% else %}
                <p class="product__card--price">
                  {% if product.compare_at_price_max > product.price %}
                    <span
                      ><s>{{ product.compare_at_price_max | money_with_currency }}</s></span
                    >
                    <span>{{ product.price | money_with_currency }}</span>
                  {% else %}
                    <span>{{ product.price | money_with_currency }}</span>
                  {% endif %}
                </p>
              {% endif %}
            </a>
          </div>
        {% endif %}
      {% endfor %}
    </div>
  </section>

  <section class="container about--content__wrapper">
    <div class="about--content">
      <p>Lorem ipsum dolor sit amet consectetur. Convallis vitae metus aliquet id tempus eget.</p>
      <a href="#">KNOW MORE</a>
    </div>
    <div class="about--image">
      <img src="{{ 'single-collection-model.png' | asset_url }}" width="460" height="530" class="" alt="">
    </div>
  </section>

  <section class="container">
    <div class="section-title">
      <h2>Recommended For You</h2>
      <div class="section-title-divider">
        <img
          src="{{ 'section-title-divider.svg' | asset_url }}"
