  <section class="container">
    <div class="section-title">
      <h2>Latest Blogs</h2>
      <div class="section-title-divider">
        <img
          src="{{ 'section-title-divider.svg' | asset_url }}"
          alt="Section Title Divider"
          width="392"
          height="40"
        >
      </div>
    </div>
    <div class="blog__lists">
      {% assign blog = blogs.article %}
      {% assign articles = blog.articles | reverse | limit: 4 %}
      {% for article in blog.articles limit: 3 %}
        <div class="blog__list">
          {% if article.image %}
            <a href="{{ article.url }}" class="blog__list--link">
              <div class="blog__list--image">
                <img
                  id="Image-{{ article.id }}"
                  loading="lazy"
                  class="{{ article_class }} blog-feed-card-image"
                  src="{{article.image.src | image_url: width:388, height: 370, crop: 'top'  }}"
                  alt="{{ article.image.alt | escape }}"
                >
              </div>
              <div class="blog__list--title">
                {{ article.title | escape }}
              </div>
              <div class="blog__list--date">
                <span>
                  {{ article.published_at | date: '%B %d, %Y' }}
                </span>
              </div>
              <div class="blog__list--content">
                {% if article.excerpt.size > 0 %}
                  {{ article.excerpt | strip_html | truncate: 90 }}
                {% else %}
                  {{ article.content | strip_html | truncate: 90 }}
                {% endif %}
              </div>
              <div class="blog__list--link-text">View More</div>
            </a>
          {% endif %}
        </div>
      {% endfor %}
    </div>
  </section>
</div>

